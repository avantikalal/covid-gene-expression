@ARTICLE{Her2017-ss,
  title    = "Severity of Plasma Leakage Is Associated With High Levels of
              Interferon {$\gamma$-Inducible} Protein 10, Hepatocyte Growth
              Factor, Matrix Metalloproteinase 2 ({MMP-2)}, and {MMP-9} During
              Dengue Virus Infection",
  author   = "Her, Zhisheng and Kam, Yiu-Wing and Gan, Victor C and Lee,
